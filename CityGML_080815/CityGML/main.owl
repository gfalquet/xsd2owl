		<rdfs:domain rdf:resource="#AppearanceType"/>
		</owl:ObjectProperty>
	<owl:DatatypeProperty rdf:about="##_GenericApplicationPropertyOfAppearance__xs_anyType">
		<rdfs:domain rdf:resource="#AppearanceType"/>
		 </owl:DatatypeProperty>
	<owl:Class rdf:ID="AppearancePropertyType">
